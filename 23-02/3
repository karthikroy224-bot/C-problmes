#include<stdio.h>
int main(){
    int seat_type;
    int show_time;

    scanf("%d",&seat_type);
    scanf("%d",&show_time);

    switch(seat_type){
        case 1:
        if(show_time>=0&&show_time<18){
        printf("ticket price= $150");
        }

        else if (show_time>=18&&show_time<=24){
            printf("ticket price= $200");
        }

        else{
            printf("invalid input");
        }
        break;

        case 2:

        if(show_time>=0&&show_time<18){
        printf("ticket price= $250");
        }

        else if (show_time>=18&&show_time<=24){
            printf("ticket price= $300");
        }

        else{
            printf("invalid input");
        }
        break;

        default:
        printf("enter valid input");
    }
    return 0;
}
