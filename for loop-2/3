#include<stdio.h>
int main(){

    int n;
    int risk_score,count=0;

    scanf("%d",&n);
    int i;
    int sum=0;
    for(i=0;i<n;i++){
        scanf("%d",&risk_score);
        sum=sum+risk_score;
         
         if(risk_score>50){
            count++;
         }
    }

    printf("Total risk : %d\n",sum);
    printf("High risk Sessions : %d",count);
    return 0;
}
