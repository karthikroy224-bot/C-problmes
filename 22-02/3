#include<stdio.h>
int main(){

    int loan_type;
    int score;

    scanf("%d",&loan_type);
    scanf("%d",&score);

    switch(loan_type){
        case 1:
        if(score>=700){
            printf("approved");
        }

        else if(score>=650&&score<=699){
            printf("manual review");
        }

        else{
            printf("rejected");
        }
        break;

        case 2:
        if(score>=700){
            printf("approved");
        }

        else{
            printf("rejected");
        }

        break;

        default:
        printf("enter valid input");
    }
    return 0;
}
