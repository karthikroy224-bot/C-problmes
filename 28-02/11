#include<stdio.h>
int main(){

    int n;
    scanf("%d",&n);
    int noise_level[n];
    int i,j;
    int max,count=0;

    for(i=0;i<n;i++){
        scanf("%d",&noise_level[i]);

        if(noise_level[i]>70){
            count++;
        }
    }

    max=noise_level[0];
    for(j=1;j<n;j++){
        if(max<noise_level[j]){
            max=noise_level[j];
        }
    }

    printf("Maximum Noise : %d\n",max);
    printf("Noise Period : %d",count);
    return 0;
}
