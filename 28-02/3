#include<stdio.h>
int main(){

    int days,patients,sum,count=0;
    scanf("%d",&days);
    int i;

    for(i=0;i<days;i++){
        scanf("%d",&patients);
        sum+=patients;
    
    
    if(patients>100){
        count++;
    }
    }

    printf("Total patients : %d\n",sum);
    printf("Over crowded Days : %d",count);
    return 0;
}
