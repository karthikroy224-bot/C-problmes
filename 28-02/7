#include<stdio.h>
int main (){

    int n;
    scanf("%d",&n);
    int voltage[n],i,j,low,count=0;

    for(i=0;i<n;i++){
        scanf("%d",&voltage[i]);

        if(voltage[i]<210){
            count++;
        }
    }

    low=voltage[0];
    for(j=1;j<n;j++){
        if(low>voltage[j]){
            low=voltage[j];
        }
    }


    printf("Minimum voltage : %d\n",low);
    printf("Low voltage events : %d", count);

    return 0;
}
