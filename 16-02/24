#include<stdio.h>
int main (){
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);

    if(a>b&&a>c){
        printf("A is greater");
    }

    else if (b>a&&b>c){
        printf("B is greater");
    }

    else if (c>a&&c>b){
        printf("C is greater");
    }

    else{
        printf("all 3 values are equal");
    }
    return 0;
}   
